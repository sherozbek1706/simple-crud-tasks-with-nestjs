// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "postgresql"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Tasks {
  id     Int      @default(autoincrement()) @id
  name   String
  isDone Boolean? @default(false)
}